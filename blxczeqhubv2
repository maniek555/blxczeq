Frame.Position = UDim2.new(0.5, -175, 0.5, -150)
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.Active = true
Frame.Draggable = true -- Enabling drag for entire frame
Frame.Parent = ScreenGui

local DragBar = Instance.new("Frame")
