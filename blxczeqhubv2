local textLabel = Instance.new("TextLabel")
textLabel.Parent = screenGui
textLabel.Text = "CREATED BY MANIEK"
textLabel.Size = UDim2.new(0, 600, 0, 75)  -- YazÄ±nÄ±n geniÅŸliÄŸini ve yÃ¼ksekliÄŸini bÃ¼yÃ¼ttÃ¼k
textLabel.Position = UDim2.new(0.5, -300, 0.5, -37.5)  -- EkranÄ±n tam ortasÄ±na yerleÅŸtirir
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)  -- BaÅŸlangÄ±Ã§ta beyaz renk
textLabel.BackgroundTransparency = 1  -- Arka planÄ± tamamen ÅŸeffaf yapar
textLabel.TextSize = 70  -- YazÄ± boyutunu bÃ¼yÃ¼ttÃ¼k
textLabel.Font = Enum.Font.SourceSansBold

local startTime = tick()  -- ZamanÄ± kaydeder

-- Renk deÄŸiÅŸtiren fonksiyon
local function changeColor()
    local elapsedTime = tick() - startTime
    local r = math.abs(math.sin(elapsedTime * 10)) * 255
    local g = math.abs(math.cos(elapsedTime * 10)) * 255
    local b = math.abs(math.sin(elapsedTime * 15)) * 255
    textLabel.TextColor3 = Color3.fromRGB(r, g, b)
end

-- Renk deÄŸiÅŸimi ve yok olma iÅŸlemi
local function onUpdate()
    changeColor()

    -- 2 saniye sonra yok olma
    if tick() - startTime >= 2 then
        screenGui:Destroy()
    end
end

game:GetService("RunService").Heartbeat:Connect(onUpdate)
wait(0.8)

